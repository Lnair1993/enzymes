function [ SQs ] = ShapeVariation(  )
    x = 0;
    x_inc = 3;
    y_inc = 3;
    z = 3;
    figure;
    hold on;
    colors = {'.r','.g','.b','.m'};
    for eps1=.1:.5:2
        x = x + x_inc;
        y = 0;
        ix_color = 0;
        for eps2=.1:.5:2
            y = y + y_inc;
            ix_color = ix_color + 1;
            superellipsoid( [1 1 1 eps1 eps2 0 0 0 0 0 0 0 x y z], 1);
        end
    end
    hold off;
end

